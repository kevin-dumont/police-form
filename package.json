{
  "name": "@sygmaa/traveler-form",
  "version": "1.0.0",
  "description": "An app for travelers",
  "main": "index.js",
  "repository": "git@github.com:kevin-dumont/police-form.git",
  "author": "KÃ©vin Dumont <kevin_dumont.external@discovery.com>",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "api:build": "lerna run build --ignore @sygmaa/web",
    "api:clear": "rimraf packages/api/**/build",
    "api:s3:sync": ". scripts/bash/load-env.sh && aws s3 sync infrastructure/$NODE_ENV/build s3://traveler-form-terraform/$NODE_ENV/build",
    "api:swagger:build": ". scripts/bash/load-env.sh && scripts/bash/api-swagger-build.sh $NODE_ENV",
    "api:test": "yarn jest --config packages/api/jest.config.js",
    "api:zip": ". scripts/bash/load-env.sh && scripts/bash/api-zip.sh $NODE_ENV",
    "cache:clear": "rimraf cache",
    "clear": "yarn cache:clear rimraf node_modules",
    "tf:apply": ". scripts/bash/load-env.sh && terraform -chdir=infrastructure/$NODE_ENV apply -auto-approve build/terraform.out",
    "tf:init": ". scripts/bash/load-env.sh && terraform -chdir=infrastructure/$NODE_ENV init",
    "tf:plan": ". scripts/bash/load-env.sh && terraform -chdir=infrastructure/$NODE_ENV plan -out=build/terraform.out -parallelism=100",
    "tf:validate": ". scripts/bash/load-env.sh && terraform -chdir=infrastructure/$NODE_ENV validate",
    "web:build": ". scripts/bash/load-env.sh && lerna run build --scope @sygmaa/web",
    "web:s3:sync": ". scripts/bash/load-env.sh && aws s3 sync ./packages/web/build s3://$WEB_BUCKET_NAME --delete",
    "web:start": ". scripts/bash/load-env.sh && lerna run start --scope @sygmaa/web",
    "web:test": "echo 'TODO'",
    "lint": "eslint ./packages",
    "lint:fix": "eslint --fix ./packages"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@types/uuid": "^9.0.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1382.0",
    "child_process": "^1.0.2",
    "dayjs": "^1.11.7",
    "esbuild": "^0.17.19",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "lerna": "^6.6.2",
    "libphonenumber-js": "^1.10.31",
    "merge-yaml": "^1.1.0",
    "merge-yaml-cli": "^1.1.2",
    "prettier": "^2.8.8",
    "swagger-merger": "^1.5.4",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0"
  }
}
